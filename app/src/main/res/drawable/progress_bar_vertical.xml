<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="@color/red" android:endColor="#ff747674" android:angle="180" android:centerY="0.75" android:centerColor="@color/red"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="180" android:centerY="0.75" android:centerColor="@color/white"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="180" android:centerY="0.75" android:centerColor="@color/white"/>
            </shape>
        </clip>
    </item>
</layer-list>
